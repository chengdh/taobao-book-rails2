<h1>New item</h1>

<% form_for(@item) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :iid %><br />
    <%= f.text_field :iid %>
  </p>
  <p>
    <%= f.label :num_iid %><br />
    <%= f.text_field :num_iid %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', items_path %>